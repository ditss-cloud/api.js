{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "cleanUrls": true,
  "rewrites": [
    { "source": "/download", "destination": "/download.html" },
    { "source": "/pulsa", "destination": "/pulsa.html" },
    { "source": "/panel", "destination": "/panel.html" },
    { "source": "/about", "destination": "/about.html" },
    { "source": "/app-prem", "destination": "/app-prem.html" },
    { "source": "/kuota", "destination": "/kuota.html" },
    { "source": "/voucher", "destination": "/voucher.html" },
    { "source": "/nokos", "destination": "/nokos.html" },
    { "source": "/vps", "destination": "/vps.html" }
  ]
}
